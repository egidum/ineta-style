<div class="col-4">
   <div
      id="carouselExampleIndicators"
      class="carousel slide"
      data-ride="carousel"
   >
      <div class="carousel-inner">
         {% for image in images %}           
         <div class="carousel-item {% if image.status === 'active' %} active {% endif %}">                        
            <img
               src="{{ image.src }}"
               class="d-block w-100"
               alt="{{ image.alt }}"
            />                       
         </div>           
         {% endfor %}         
      </div>
      <a
         class="carousel-control-prev"
         href="#carouselExampleIndicators"
         role="button"
         data-slide="prev"
      >
         <span
         class="carousel-control-prev-icon"
         aria-hidden="true"
         ></span>
         <span class="sr-only">Previous</span>
      </a>
      <a
         class="carousel-control-next"
         href="#carouselExampleIndicators"
         role="button"
         data-slide="next"
      >
         <span
         class="carousel-control-next-icon"
         aria-hidden="true"
         ></span>
         <span class="sr-only">Next</span>
      </a>
   </div>
</div>
<!-- Carousel [end]-->
